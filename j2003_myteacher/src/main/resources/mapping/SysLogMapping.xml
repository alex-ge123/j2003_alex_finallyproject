<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qianfeng.j2003.mapper.SysLogMapper">
    <select id="insertSysLog" parameterType="com.qianfeng.j2003.pojo.SysLog">
        INSERT into tb_syslog (userName,userAction,createTime)VALUES (#{userName},#{userAction},#{createTime})
    </select>
</mapper>
